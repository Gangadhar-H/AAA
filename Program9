package practice;

import java.util.Scanner;

class Storage<T> {
	private T item;

	public T getItem() {
		return item;
	}

	public void setItem(T item) {
		this.item = item;
	}
	
	
}

public class Program9 {
	
	public static <E> void printArray (E[] arr) {
		for (E e: arr) {
			System.out.print(e + " ");
		}
		System.out.println();
	}

	public static void main(String[] args) {
		
		Storage<Integer> s1 = new Storage<>();
		
		s1.setItem(10);
		System.out.println("Integer item: " + s1.getItem());
		
		Storage<String> s2 = new Storage<>();
		s2.setItem("Book");
		System.out.println("Integer item: " + s2.getItem());
		
		Integer[] arrInt = {1, 2, 3};
		Program9.printArray(arrInt);
		String[] arrString = {"Java", "WAD", "CN"};
		Program9.printArray(arrString);
		
	}

}


